.review_form
  %fieldset
    = form_for [@car_wash, @review], html: { multipart: true } do |f|
      = hidden_field_tag "review[rating]", nil, class: 'rating_field'
      .m-row
        = f.text_field :text
      .m-row
        = f.fields_for :images, @review.images.build do |is|
          = is.label :image
          = is.file_field :image

      -#.uploader
        
      .m-row
        = f.submit 'Отправить'
