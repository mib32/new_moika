%table#car_wash_info.m-info-table
  %tr
    %td.td-10
      %span.title Адрес:
    %td
      %span= car_wash.address
  %tr
    %td.td-10
      %span.title Сайт:
    %td
      %span= link_to car_wash.site_url, url_with_protocol(car_wash.site_url), :target => '_blank'
  %tr
    %td.td-10
      %span.title Контакты:
    %td
      %span= car_wash.contacts
  %tr
    %td.td-10
      %span.title Услуги:
    %td
      %span= car_wash.services
  %tr
    %td.td-10
      %span.title Цена:
    %td
      %span= car_wash.price
  %tr
    %td.td-10
      %span.title Кол-во моечных зон:
    %td
      %span= car_wash.zones_count
  %tr
    %td.td-10
      %span.title Акция:
    %td
      -#%ul
        -# = actions(left_actions, 'li')
      = car_wash.widget_content
