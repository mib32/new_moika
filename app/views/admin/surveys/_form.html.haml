= form_for @admin_survey do |f|
  - if @admin_survey.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_survey.errors.count, "error")} prohibited this admin_survey from being saved:"
      %ul
        - @admin_survey.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :question
    = f.text_field :question
  .field
    = f.label :answer_1
    = f.text_field :answer_1
  .field
    = f.label :answer_2
    = f.text_field :answer_2
  .field
    = f.label :answer_3
    = f.text_field :answer_3
  .field
    = f.label :answer_4
    = f.text_field :answer_4
  .field
    = f.label :answer_5
    = f.text_field :answer_5
  .actions
    = f.submit 'Save'
